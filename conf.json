{
  "version": "2025.10.detailed",
  "enable_nmap": true,
  "enable_http_probe": true,
  "run_global_sslscan": false,
  "parallel_workers": 6,
  "nmap": {
    "scan_all_tcp_ports": true,
    "use_service_detection": true,
    "os_detection": false,
    "timing_template": 4,
    "safe_mode": true
  },
  "timeouts": {
    "nmap": 900,
    "curl": 8,
    "nikto": 600,
    "nuclei": 600,
    "sslscan": 120,
    "hydra": 300,
    "binwalk": 300
  },
  "tools": {
    "nmap": { "enabled": true },
    "nuclei": { "enabled": true, "severity": ["high","critical"] },
    "nikto": { "enabled": true },
    "sslscan": { "enabled": true },
    "hydra": { "enabled": false, "userlist": "user.txt", "passlist": "pass.txt" },
    "binwalk": { "enabled": false, "firmware_path": "firmware.bin" }
  },
  "report": {
    "max_output_chars": 20000,
    "include_stdout": true,
    "html_theme": "simple"
  }
}
